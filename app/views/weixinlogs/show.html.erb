<%- model_class = Weixinlog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @weixinlog.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:openid) %>:</strong></dt>
  <dd><%= @weixinlog.openid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:optquery) %>:</strong></dt>
  <dd><%= @weixinlog.optquery %></dd>
  <dt><strong><%= model_class.human_attribute_name(:optreplay) %>:</strong></dt>
  <dd><%= @weixinlog.optreplay %></dd>
  <dt><strong><%= model_class.human_attribute_name(:opttime) %>:</strong></dt>
  <dd><%= @weixinlog.opttime %></dd>
  <dt><strong><%= model_class.human_attribute_name(:marketdate) %>:</strong></dt>
  <dd><%= @weixinlog.marketdate %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              weixinlogs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_weixinlog_path(@weixinlog), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              weixinlog_path(@weixinlog),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
