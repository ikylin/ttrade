<div class="container">
      
          <%- model_class = Marketdate -%>
          <div class="page-header">
            <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
          </div>


   <div class="row" >
      <div class="col-lg-12 col-md-offset-0"> 

        <ul class="nav nav-pills" role="tablist">
          <li role="presentation" class="active"><a href="#r_a" role="tab" data-toggle="tab">A<span class="badge">1</span></a></li>
          <li role="presentation"><a href="#r_g" role="tab" data-toggle="tab">G<span class="badge">2</span></a></li>
          <li role="presentation"><a href="#r_m" role="tab" data-toggle="tab">M<span class="badge">2</span></a></li>
        </ul>

        <div class="tab-content">
          <div role="tabpanel" class="tab-pane active" id="r_a">
            <div class="panel panel-primary">
              <div class="panel-heading">【A股交易日历】</div>
              <div class="panel-body">

   
                <ul class="pagination">
                  <%= will_paginate @marketdates_A %>
                </ul>

                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th><%= model_class.human_attribute_name(:id) %></th>
                      <th><%= model_class.human_attribute_name(:tag) %></th>
                      <th><%= model_class.human_attribute_name(:tradedate) %></th>
                      <th><%= model_class.human_attribute_name(:daystate) %></th>
                      <th><%=t '.actions', :default => t("helpers.actions") %></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @marketdates_A.each do |marketdate| %>
                      <tr>
                        <td><%= link_to marketdate.id, marketdate_path(marketdate) %></td>
                        <td><%= marketdate.tag %></td>
                        <td><%= marketdate.tradedate %></td>
                        <td><%= marketdate.daystate %></td>
                        <td>
                          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                                      edit_marketdate_path(marketdate), :class => 'btn btn-default btn-xs' %>
                          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                                      marketdate_path(marketdate),
                                      :method => :delete,
                                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                                      :class => 'btn btn-xs btn-danger' %>
                        </td>
                      </tr>
                    <% end %>
                  </tbody>
                </table>

                <ul class="pagination">
                  <%= will_paginate @marketdates_A %>
                </ul>

                <%= link_to t('.new', :default => t("helpers.links.new")),
                            new_marketdate_path,
                            :class => 'btn btn-primary' %>
 

              </div>
            </div>
          </div>
         
